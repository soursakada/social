{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "caption": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "private",
        "public",
        "team"
      ]
    },
    "poster": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::poster.poster"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "post"
    },
    "reactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reaction.reaction",
      "mappedBy": "post"
    }
  }
}
